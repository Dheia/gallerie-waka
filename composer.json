{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "barryvdh/laravel-debugbar": "^3.7",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "jessarcher/laravel-castable-data-transfer-object": "^2.2",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.16",
        "laravel/sanctum": "^3.0",
        "laravel/tinker": "^2.7",
        "protonemedia/inertiajs-tables-laravel-query-builder": "^2.0",
        "spatie/eloquent-sortable": "^4.0",
        "spatie/laravel-medialibrary": "10.2.*",
        "spatie/laravel-query-builder": "^5.1",
        "symfony/http-client": "^6.2",
        "symfony/mailgun-mailer": "^6.2",
        "tightenco/ziggy": "^1.0",
        "waka/laravel_yamlforms": "0.0.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/attributes": "^2.2",
        "laravel-lang/lang": "^12.17",
        "laravel-lang/publisher": "^14.6",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "waka/yamlformsconsole": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../01-repo/laravel-yaml-forms"
        },
        {
            "type": "path",
            "url": "../01-repo/laravel-yaml-console"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
